{
    "name": "mttzzz/dk-role-permission-tool",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "eminiarts/nova-tabs": "^2.2",
        "spatie/laravel-permission": "^6.0",
        "pavloniym/nova-options-selector": "*"
    },
    "autoload": {
        "psr-4": {
            "Mttzzz\\DkRolePermissionTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mttzzz\\DkRolePermissionTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
